.search-result-meta-count::before { content: "💬"; }

.search-result .meta-group { align-items: center; }

.search-result-breadcrumbs { margin: 0px; }

.search-result-breadcrumbs li:last-child::after { content: "·"; display: inline-block; margin: 0px 5px; }
