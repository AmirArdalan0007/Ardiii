#hc-wysiwyg [class^="wysiwyg-icon-"] { background: transparent; border-radius: 3px; border: 1px solid transparent; color: rgb(82, 82, 82); cursor: pointer; display: inline-block; font-size: 14px; font-weight: normal; font-style: normal; height: 24px; line-height: 23px; margin: 0px 3px; padding: 0px 5px; text-align: center; vertical-align: middle; min-width: 24px; width: auto; }

#hc-wysiwyg [class^="wysiwyg-icon-"]:hover, #hc-wysiwyg [class^="wysiwyg-icon-"][aria-selected="true"] { border-color: rgb(209, 209, 209); }

#hc-wysiwyg [class^="wysiwyg-icon-"]:hover { background: rgb(255, 255, 255); }

#hc-wysiwyg [class^="wysiwyg-icon-"]:active, #hc-wysiwyg [class^="wysiwyg-icon-"][aria-selected="true"] { background: rgb(221, 221, 221); }

#hc-wysiwyg [role="toolbar"] > [aria-haspopup="true"] { position: relative; }

#hc-wysiwyg [role="menu"] { position: absolute; background: rgb(255, 255, 255); border: 1px solid rgb(221, 221, 221); border-radius: 3px; box-shadow: rgba(0, 0, 0, 0.1) 0px 0px 5px; left: 0px; top: 100%; margin-top: 8px; min-width: 150px; padding: 5px 0px; outline: none; }

#hc-wysiwyg [role="menu"][aria-hidden="true"] { display: none; }

[dir="rtl"] #hc-wysiwyg [role="menu"] { left: auto; right: 0px; }

#hc-wysiwyg [role="menu"] > [role="button"] { background: rgb(255, 255, 255); border: 0px; border-radius: 0px; color: inherit; display: block; margin: 0px; padding: 8px 20px; text-align: initial; white-space: nowrap; height: auto !important; line-height: 1 !important; }

#hc-wysiwyg [role="menu"] > [role="button"]:hover { background: rgb(245, 245, 245); }

#hc-wysiwyg [role="menu"] > [role="button"][data-block="p"] { font: 12px sans-serif; }

#hc-wysiwyg [role="menu"] > [role="button"][data-block="code"] { font: 12px monospace; word-spacing: 2px; }

#hc-wysiwyg [role="menu"] > [role="button"][data-block="h2"] { font: bold 16px sans-serif; }

#hc-wysiwyg [role="menu"] > [role="button"][data-block="h3"] { font: bold 14px sans-serif; }

#hc-wysiwyg [role="menu"] > [role="button"][data-block="h4"] { font: bold 13px sans-serif; }

#hc-wysiwyg [role="menu"] > [role="button"][aria-selected="true"]::after { content: "✓"; display: inline-block; margin-left: 20px; font-family: entypo; font-size: 12px; font-weight: normal; }

[dir="rtl"] #hc-wysiwyg [role="menu"] > [role="button"][aria-selected="true"]::after { margin-left: 0px; margin-right: 20px; }

#hc-wysiwyg [role="separator"] { border: 0px; background: rgb(208, 208, 208); width: 1px; height: 24px; display: inline-block; margin: 0px 10px; vertical-align: middle; }

#hc-wysiwyg .wysiwyg-icon-image { overflow: hidden; position: relative; }

#hc-wysiwyg input[type="file"] { font-size: 60px; height: 28px; position: absolute; top: 0px; right: 0px; z-index: 11; cursor: pointer; opacity: 0; }

#hc-wysiwyg [aria-dropeffect] { background: rgb(255, 255, 255); border-radius: 2px; border: 2px solid; bottom: 0px; left: 0px; position: absolute; right: 0px; top: 0px; }

#hc-wysiwyg [aria-dropeffect]::before { content: attr(title); display: inline-block; font-size: 14px; font-weight: normal; height: 100%; position: absolute; text-align: center; top: 45%; width: 100%; }

#hc-wysiwyg [aria-dropeffect][aria-hidden="true"] { display: none; }

#hc-wysiwyg [aria-dropeffect][data-over="true"] { background: rgb(249, 249, 249); }

#hc-wysiwyg [role="form"] { background: whitesmoke; border-bottom: 1px solid rgb(236, 236, 236); padding: 3px 10px; position: absolute; height: 35px; top: 0px; transition: left 0.2s ease-in 0s; width: 100%; z-index: 20; }

#hc-wysiwyg [role="form"] * { display: inline-block; margin: 0px; padding: 0px; vertical-align: middle; }

#hc-wysiwyg [role="form"] label { font-weight: normal; }

#hc-wysiwyg [role="form"] input { border: 1px solid rgb(221, 221, 221); height: 28px; outline: none; padding: 0px 5px; width: 300px; }

#hc-wysiwyg [role="form"] [role="button"] { background: rgb(255, 255, 255); border: 1px solid rgb(197, 197, 197); color: rgb(153, 153, 153); cursor: pointer; font-size: 13px; height: 28px; line-height: 28px; padding: 0px 5px; min-width: 45px; text-align: center; }

#hc-wysiwyg [role="form"] [role="button"]:last-child { background: transparent; border-color: transparent; color: rgb(187, 187, 187); font-size: 12px; }

#hc-wysiwyg [role="form"][aria-expanded="false"] { left: -100%; }

#hc-wysiwyg [role="form"][aria-expanded="true"] { left: 0px; }

#hc-wysiwyg [role="alert"] { background: rgb(255, 255, 255); border-top: 1px solid rgb(236, 236, 236); bottom: 0px; cursor: pointer; padding: 10px; position: absolute; width: 100%; }

#hc-wysiwyg [role="alert"][aria-hidden="true"] { display: none; }

.powered-by-zendesk { clear: both; text-align: right; padding: 10px 15px; height: 38px; }

.powered-by-zendesk svg { position: relative; display: inline-block; top: 3px; content: " "; width: 15px; height: 15px; margin-right: 5px; fill: rgb(165, 165, 165); }

[dir="rtl"] .powered-by-zendesk svg { margin-left: 5px; margin-right: 0px; }

.powered-by-zendesk a { color: rgb(165, 165, 165); font-family: Helvetica, Arial, sans-serif; font-size: 13px; font-weight: normal; border: 0px; text-transform: none; }

#navbar-container { font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Helvetica, Arial, sans-serif; }

.nesty-input, .nesty-panel { box-sizing: border-box; }

.nesty-input *, .nesty-input ::before, .nesty-input ::after, .nesty-panel *, .nesty-panel ::before, .nesty-panel ::after { box-sizing: inherit; }

.nesty-input { background-color: rgb(255, 255, 255); border: 1px solid rgb(236, 236, 236); border-radius: 1px; box-shadow: rgb(227, 227, 227) 0px 1px inset; color: rgb(51, 51, 51); cursor: pointer; display: inline-block; height: 30px; line-height: 30px; margin: 0px; max-width: 250px; min-width: 150px; overflow: hidden; padding: 0px 20px 0px 5px; position: relative; text-overflow: ellipsis; z-index: 1; width: 100%; white-space: nowrap; }

[dir="rtl"] .nesty-input { padding: 0px 5px 0px 20px; }

.nesty-input::after { color: rgb(153, 153, 153); content: "▾"; line-height: 1em; position: absolute; right: 10px; top: 25%; }

[dir="rtl"] .nesty-input::after { left: 10px; right: auto; }

.nesty-input:hover { text-decoration: none; }

.nesty-panel, .nesty-panel ul { overflow: hidden auto; }

.nesty-panel { background: rgb(255, 255, 255); border: 1px solid rgb(170, 170, 170); box-shadow: rgba(0, 0, 0, 0.15) 0px 2px 5px; height: 150px; min-width: 100px; z-index: 999; position: absolute !important; }

.nesty-panel ul { background: rgb(255, 255, 255); border: 0px; bottom: 0px; height: 100%; left: 0px; margin: 0px; padding: 0px; position: absolute; right: 0px; top: 0px; transform: translate3d(0px, 0px, 0px); backface-visibility: hidden; width: 100% !important; }

.nesty-panel li { background: rgb(255, 255, 255); border: 0px; color: rgb(51, 51, 51); cursor: pointer; display: block; font-size: 13px; font-weight: normal; font-style: normal; font-family: sans-serif; line-height: 1; min-height: 27px; position: relative; box-sizing: border-box; margin: 0px !important; padding: 5px !important; }

.nesty-panel li:focus { background: rgb(233, 233, 233); outline: none; }

.nesty-panel li.nesty-selected { font-weight: bold; }

li.nesty-collapse { color: rgb(153, 153, 153); }

.nesty-expand::after, .nesty-collapse::after { border-color: transparent; border-style: solid; border-width: 4px; content: " "; height: 0px; margin-top: -5px; position: absolute; right: 5px; top: 50%; width: 0px; }

[dir="rtl"] .nesty-expand::after, [dir="rtl"] .nesty-collapse::after { left: 5px; right: auto; }

.nesty-expand::after { border-left-color: rgb(153, 153, 153); border-right-width: 0px; }

[dir="rtl"] .nesty-expand::after { border-left-color: transparent; border-left-width: 0px; border-right-color: rgb(153, 153, 153); border-right-width: 4px; }

.nesty-collapse::after { border-left-width: 0px; border-right-color: rgb(153, 153, 153); }

[dir="rtl"] .nesty-collapse::after { border-left-color: rgb(153, 153, 153); border-left-width: 4px; border-right-color: transparent; border-right-width: 0px; }

.nesty-input { border: 1px solid rgb(221, 221, 221); border-radius: 2px; box-shadow: none; padding: 0px 15px 0px 10px; }

.nesty-panel { height: auto; max-height: 150px; }

.nesty-panel ul { position: relative; }

.nesty-panel ul:nth-child(2) { position: absolute; }

body { }

body .pika-single { z-index: 9999; display: block; position: relative; width: 240px; padding: 8px; color: rgb(51, 51, 51); background: rgb(255, 255, 255); border-width: 1px; border-style: solid; border-color: rgb(204, 204, 204) rgb(204, 204, 204) rgb(187, 187, 187); border-image: initial; font-family: "Helvetica Neue", Helvetica, Arial, sans-serif; }

body .pika-single.is-hidden { display: none; }

body .pika-single.is-bound { position: absolute; box-shadow: rgba(0, 0, 0, 0.5) 0px 5px 15px -5px; }

body .pika-title { position: relative; text-align: center; }

body .pika-label { display: inline-block; position: relative; z-index: 9999; overflow: hidden; margin: 0px; padding: 5px 3px; font-size: 14px; line-height: 20px; font-weight: bold; background-color: rgb(255, 255, 255); }

body .pika-title select { cursor: pointer; position: absolute; z-index: 9998; margin: 0px; left: 0px; top: 5px; opacity: 0; }

body .pika-prev, body .pika-next { display: block; cursor: pointer; position: relative; outline: none; border: 0px; padding: 0px; width: 20px; height: 30px; background-color: transparent; background-position: center center; background-repeat: no-repeat; background-size: 75% 75%; white-space: nowrap; text-indent: 100%; overflow: hidden; opacity: 0.5; }

body .pika-prev:hover, body .pika-next:hover { opacity: 1; }

body .pika-prev, body .is-rtl .pika-next { float: left; background-image: url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABQAAAAeCAYAAAAsEj5rAAAAUklEQVR42u3VMQoAIBADQf8Pgj+OD9hG2CtONJB2ymQkKe0HbwAP0xucDiQWARITIDEBEnMgMQ8S8+AqBIl6kKgHiXqQqAeJepBo/z38J/U0uAHlaBkBl9I4GwAAAABJRU5ErkJggg=="); }

body .pika-next, body .is-rtl .pika-prev { float: right; background-image: url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABQAAAAeCAYAAAAsEj5rAAAAU0lEQVR42u3VOwoAMAgE0dwfAnNjU26bYkBCFGwfiL9VVWoO+BJ4Gf3gtsEKKoFBNTCoCAYVwaAiGNQGMUHMkjGbgjk2mIONuXo0nC8XnCf1JXgArVIZAQh5TKYAAAAASUVORK5CYII="); }

body .pika-prev.is-disabled, body .pika-next.is-disabled { cursor: default; opacity: 0.2; }

body .pika-select { display: inline-block; }

body .pika-table { width: 100%; border-collapse: collapse; border-spacing: 0px; border: 0px; }

body .pika-table th, body .pika-table td { width: 14.2857%; }

body .pika-table th { color: rgb(153, 153, 153); font-size: 12px; line-height: 25px; font-weight: bold; text-align: center; }

body .pika-button { cursor: pointer; display: block; outline: none; border: 0px; margin: 0px; width: 100%; padding: 5px; color: rgb(102, 102, 102); font-size: 12px; line-height: 15px; text-align: right; background: rgb(245, 245, 245); }

body .is-today .pika-button { color: rgb(51, 170, 255); font-weight: bold; }

body .is-selected .pika-button { color: rgb(255, 255, 255); font-weight: bold; background: rgb(51, 170, 255); box-shadow: rgb(23, 143, 229) 0px 1px 3px inset; border-radius: 3px; }

body .is-disabled .pika-button { pointer-events: none; cursor: default; color: rgb(153, 153, 153); opacity: 0.3; }

body .pika-button:hover { color: rgb(255, 255, 255) !important; background: rgb(255, 128, 0) !important; box-shadow: none !important; border-radius: 3px !important; }

body .pika-single.is-bound { border: 1px solid rgb(209, 209, 209); border-radius: 2px; box-shadow: rgba(0, 0, 0, 0.15) 0px 1px 5px; }

body .pika-single button { box-shadow: none; border-radius: 0px; height: auto; text-align: center; }

body .pika-table th, body .pika-table td { border: 0px; padding: 0px; }

body .pika-table abbr { border: 0px; }

body .pika-button { background: rgb(255, 255, 255); }

body .pika-button:hover { color: rgb(51, 51, 51) !important; background: rgb(245, 245, 245) !important; box-shadow: none !important; border-radius: 0px !important; }

body .is-today .pika-button { color: rgb(0, 0, 0); }

body .is-selected .pika-button { background: rgb(204, 204, 204); box-shadow: none; border-radius: 0px; }

body .pika-select { padding: 0px; }

html[dir="rtl"] body .pika-title select { left: 0px; }

html[dir="rtl"] body .pika-prev, html[dir="rtl"] body .is-rtl .pika-next { float: left; }

html[dir="rtl"] body .pika-next, html[dir="rtl"] body .is-rtl .pika-prev { float: right; }

html[dir="rtl"] body .pika-button { text-align: right; }
