@media (min-width: 768px) {
  .profile-activity:not(:last-child) { border-left: 1px solid rgb(221, 221, 221); }
  [dir="rtl"] .profile-activity:not(:last-child) { border-left: 0px; border-right: 1px solid rgb(221, 221, 221); }
}

.profile-activity-header { display: flex; align-items: center; margin-left: 35px; }

[dir="rtl"] .profile-activity-header { margin-left: 0px; margin-right: 35px; }

