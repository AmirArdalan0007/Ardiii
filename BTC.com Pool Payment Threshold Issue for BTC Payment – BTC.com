  [dir="rtl"] .profile-activity-header { margin-right: 0px; }
}

.profile-activity-header .user-avatar { width: 40px; height: 40px; margin-right: 10px; min-width: 40px; align-self: flex-start; }

[dir="rtl"] .profile-activity-header .user-avatar { margin-left: 10px; margin-right: 0px; }

.profile-activity-description { word-break: break-word; overflow-wrap: break-word; margin: 0px; min-width: 0px; width: 100%; }

.profile-activity-description span:first-child { font-weight: 600; display: inline; }

.profile-activity-contribution { padding: 20px; margin-top: 10px; border-radius: 8px; background-color: rgb(247, 247, 247); }

@media (min-width: 768px) {
  .profile-activity-contribution { margin-top: 0px; margin-left: 50px; }
  [dir="rtl"] .profile-activity-contribution { margin-left: 0px; margin-right: 50px; }
}

.profile-activity::before { position: absolute; left: 0px; width: 28px; border-radius: 50%; content: ""; background-size: 14px 14px; background-repeat: no-repeat; background-color: rgb(255, 255, 255); background-position: 50% 50%; text-align: center; line-height: 40px; font-size: 16px; color: rgb(204, 204, 204); font-family: copenhagen-icons; }

[dir="rtl"] .profile-activity::before { right: 0px; }

@media (min-width: 768px) {
