.notification { border: 1px solid; display: table; font-family: sans-serif; font-size: 12px; padding: 13px 15px; transition: height 0.2s ease 0s; width: 100%; color: rgb(85, 85, 85); }

.notification a { color: rgb(21, 142, 194); }

.notification-inner { margin: 0px auto; padding: 0px 20px; max-width: 980px; }

.notification-icon, .notification-text, .notification-dismiss { display: table-cell; vertical-align: middle; }

.notification-text { padding: 0px 15px; width: 100%; }

.notification + .notification { margin-bottom: -1px; position: relative; top: -1px; }

.notification-error { background: rgb(255, 237, 237); border-color: rgb(247, 203, 203); }

.notification-error .notification-icon::before { border-color: rgb(247, 203, 203); content: "!"; font-family: sans-serif; font-size: 14px; font-weight: bold; }

.notification-notice { background: rgb(219, 243, 255); border-color: rgb(181, 224, 245); }

.notification-notice .notification-icon::before { border-color: rgb(181, 224, 245); content: "✓"; }

.notification-alert, .notification-lock { color: rgb(173, 94, 24); background: rgb(255, 248, 237); border-color: rgb(252, 219, 169); }

.notification-alert .notification-icon::before, .notification-lock .notification-icon::before { border-color: rgb(237, 229, 161); }

.notification-alert .notification-icon::before { content: "ℹ"; font-size: 10px; }

.notification-lock .notification-icon::before { content: "🔒"; font-size: 12px; }

.notification-icon::before { border: 1px solid; border-radius: 50%; font-family: entypo; }

.notification-dismiss { cursor: pointer; }

.notification-dismiss::before { color: initial; content: "✕"; font-family: sans-serif; font-size: 16px; opacity: 0.6; transition: opacity 100ms ease 0s; text-decoration: none !important; }

.notification-dismiss:hover::before { opacity: 1; }

.notification-icon::before, .notification-dismiss::before { display: inline-block; line-height: 20px; text-align: center; width: 20px; }

.notification-inline { border-radius: 4px; line-height: 14px; margin-top: 5px; padding: 5px; position: relative; text-align: left; vertical-align: middle; }

[dir="rtl"] .notification-inline { text-align: right; }

.notification-inline[aria-hidden="true"] { display: none; }

.notification-inline.notification-error::before { border-radius: 50%; border: 1px solid rgb(227, 91, 102); color: rgb(227, 91, 102); content: "!"; display: inline-block; font-family: sans-serif; font-size: 8px; font-weight: normal; height: 12px; line-height: 12px; margin: -2px 5px 0px 0px; text-align: center; vertical-align: middle; width: 12px; }

[dir="rtl"] .notification-inline.notification-error::before { margin: 0px 0px 0px 5px; }

.notification-inline.notification-error { background-color: rgb(255, 240, 241); border: 1px solid rgb(227, 91, 102); color: rgb(204, 51, 64); }

.notification-inline.notification-large { padding: 13px 15px; margin-bottom: 25px; }

.notification-left-aligned { text-align: left; padding-left: 0px; }

html[dir="rtl"] .notification-left-aligned { text-align: right; padding-right: 0px; }

.g-recaptcha[ui="invisible"] { margin-top: 20px; }

.g-recaptcha[ui="invisible"] .grecaptcha-badge { display: inline-block; }

.satisfaction-box { background-color: rgb(251, 251, 251); border: 1px solid rgb(236, 236, 236); border-radius: 4px; margin-top: 30px; margin-bottom: 40px; padding: 8px 30px 10px; }

.satisfaction-box h4 { font-weight: bold; }

.satisfaction-box h4.success { margin: 0px; }

.satisfaction-box textarea { border-radius: 4px; margin-bottom: 30px; resize: none; }

.satisfaction-box a:hover { text-decoration: none; }

.satisfaction-box .satisfaction-comment, .satisfaction-box .satisfaction-reason { display: none; }

.satisfaction-box input[type="radio"] { display: none; }

.satisfaction-box input[type="radio"][checked="checked"] ~ div.satisfaction-comment { display: block; }

.satisfaction-box input[type="radio"][checked="checked"][value="bad"] ~ div.satisfaction-reason { display: block; margin-bottom: 15px; }

.satisfaction-box input[type="radio"][value="good"] + label:hover { border-color: rgb(134, 172, 27); color: rgb(134, 172, 27); }

.satisfaction-box input[type="radio"][value="bad"] + label:hover { border-color: rgb(227, 80, 70); color: rgb(227, 80, 70); }

.satisfaction-box input[type="radio"] + label.disabled { cursor: default; }

.satisfaction-box input[type="radio"][checked="checked"][value="good"] + label { border-color: rgb(94, 132, 0); background-color: rgb(120, 163, 0); color: rgb(255, 255, 255); }

.satisfaction-box input[type="radio"][checked="checked"][value="bad"] + label { border-color: rgb(191, 28, 17); background-color: rgb(224, 59, 48); color: rgb(255, 255, 255); }

.satisfaction-box input[type="radio"] ~ label { background: rgb(255, 255, 255); border: 1.5px solid rgb(236, 236, 236); border-radius: 4px; color: rgb(120, 120, 120); font-size: 11px; letter-spacing: 1px; margin-right: 20px; margin-bottom: 15px; padding: 5px 20px; height: auto; }

.satisfaction-box input[type="submit"] { margin-left: 25px; font-size: 11px; }

.satisfaction-box .satisfaction-link-box { text-align: right; }

.satisfaction-box .satisfaction-link-box a { margin-left: 25px; }

.satisfaction-box .satisfaction-submit { background: rgb(68, 68, 68); color: rgb(255, 255, 255); display: inline-block; margin-right: 0px; }

.satisfaction-box .satisfaction-cancel { font-size: 11px; line-height: 27px; }

html[dir="rtl"] .satisfaction-box input[type="radio"] ~ label { margin-right: auto; margin-left: 20px; }

html[dir="rtl"] .satisfaction-box input[type="submit"] { margin-left: auto; margin-right: 25px; }

html[dir="rtl"] .satisfaction-box .satisfaction-link-box { text-align: left; }

html[dir="rtl"] .satisfaction-box .satisfaction-link-box a { margin-left: auto; margin-right: 25px; }

html[dir="rtl"] .satisfaction-box .satisfaction-submit { margin-right: auto; margin-left: 0px; }

.upload-pool { margin: 0px; overflow: hidden; padding: 0px; }

.upload-item { border-radius: 3px; clear: left; float: left; font-size: 12px; margin-top: 10px; overflow: hidden; padding: 5px; position: relative; width: 50%; }

.upload-item[aria-busy="true"] .upload-remove { display: none; }

.upload-item[aria-busy="true"] .upload-link { color: rgb(102, 102, 102); }

.upload-item[aria-busy="true"] .upload-progress { visibility: visible; }

.upload-item::after { content: ""; display: block; }

.upload-item:hover { background: rgb(242, 242, 242); }

.upload-item + .upload-item { margin-top: 5px; }

.upload-item > a, .upload-item span { position: relative; z-index: 1; }

.upload-item .upload-link { color: rgb(51, 51, 51); display: inline-block; max-width: calc(100% - 30px); overflow: hidden; text-decoration: none; text-overflow: ellipsis; vertical-align: middle; white-space: nowrap; }

.upload-item .upload-link::before { color: rgb(136, 136, 136); content: "📎"; font-family: entypo; margin-right: 5px; }

.upload-item .upload-link:hover { text-decoration: none; }

.upload-item .upload-remove { color: rgb(136, 136, 136); cursor: pointer; float: right; font-weight: bold; margin: 0px 5px 0px 15px; }

.upload-item .upload-remove:hover::before { color: rgb(34, 34, 34); }

.upload-item .upload-remove::before { content: "×"; font-family: entypo; }

.upload-item .upload-path { display: none !important; }

.upload-item .upload-progress { background-color: rgb(0, 114, 239); height: 2px; transition: width 0.5s linear 0s; visibility: hidden; width: 5%; }

.upload-item-screencast .upload-progress { width: 100%; }

#upload-error { display: none; margin-top: 10px; }

html[dir="rtl"] .upload-item { clear: right; float: right; }

html[dir="rtl"] .upload-item .upload-link { margin-left: auto; margin-right: 20px; }

html[dir="rtl"] .upload-item .upload-link::before { left: auto; right: -20px; margin-right: auto; margin-left: 5px; }

html[dir="rtl"] .upload-item .upload-remove { float: left; margin-left: auto; margin-right: 15px; }

.upload-dropzone { border: 1px solid rgb(221, 221, 221); font-size: 12px; overflow: hidden; position: relative; text-align: center; }

.upload-dropzone input[type="file"] { opacity: 0; position: absolute; top: 0px; right: 0px; cursor: pointer; font-size: 250px; height: 100%; }

.upload-dropzone span { color: rgb(209, 209, 209); display: inline-block; line-height: 24px; padding: 10px; }

.upload-dropzone span::before { content: "📎"; font-family: entypo; font-size: 20px; margin-right: 5px; position: relative; top: -2px; vertical-align: middle; }

.upload-dropzone a { text-decoration: none; font-size: 13px; }

.upload-dropzone a:hover { text-decoration: underline; }

.upload-dropzone[data-hover="true"] { background-color: rgba(0, 0, 0, 0.02); }

.upload-dropzone[data-hc-focus="true"] { border-color: rgb(153, 153, 153); }

html[dir="rtl"] .upload-dropzone input[type="file"] { right: auto; left: 0px; }

html[dir="rtl"] .upload-dropzone span::before { margin-right: auto; margin-left: 5px; }

#user #user-name { display: inline-block; max-width: 200px; overflow: hidden; text-overflow: ellipsis; vertical-align: middle; }

#user #user-dropdown { background: rgb(255, 255, 255); min-width: 200px; }

