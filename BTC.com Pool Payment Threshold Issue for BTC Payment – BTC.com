[dir="rtl"] .profile-comments .profile-contribution-breadcrumbs { padding-right: 30px; padding-left: 0px; }

@media (min-width: 768px) {
  .profile-comments .profile-contribution-breadcrumbs { margin-left: 0px; }
  [dir="rtl"] .profile-comments .profile-contribution-breadcrumbs { padding-right: 0px; }
}

.profile-section .no-activity, .profile-section .private-activity { display: block; margin-top: 40px; color: rgb(153, 153, 153); }

